{
    "name": "toxufe/yii2-file-upload-widget",
    "description": "Custom Blueimp file upload widget for the Yii framework",
    "keywords": [
        "2amigos",
        "yii",
        "yii2",
        "yii 2",
        "widget",
        "upload"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "homepage": "",
    "authors": [
        {
            "name": "Xuefei Tong",
            "email": "723887809@qq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "toxufe/yii2-gallery-widget": "~1.0",
        "bower-asset/blueimp-file-upload": "~9.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "toxufe\\fileupload\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
